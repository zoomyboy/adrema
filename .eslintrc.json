{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "overrides": [
        {
            "files": [
                "*.vue"
            ],
            "rules": {
                "vue/multi-word-component-names": "off"
            }
        }
    ]
}
